<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    tools:context=".dashboard">

    <Button
        android:id="@+id/check_fees"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/round_button"
        android:gravity="center_vertical|center_horizontal"
        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="54dp" />

    <Button
        android:id="@+id/check_results"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/round_button"
        android:gravity="center_vertical|center_horizontal"
        tools:layout_editor_absoluteX="216dp"
        tools:layout_editor_absoluteY="54dp" />

    <Button
        android:id="@+id/course_materials"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/round_button"
        android:gravity="center_vertical|center_horizontal"
        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="170dp" />

    <Button
        android:id="@+id/academic_calender"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/round_button"
        android:gravity="center_vertical|center_horizontal"
        tools:layout_editor_absoluteX="216dp"
        tools:layout_editor_absoluteY="170dp" />

    <Button
        android:id="@+id/send_message"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/round_button"
        android:gravity="center_vertical|center_horizontal"
        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="294dp" />

    <Button
        android:id="@+id/logout"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/round_button"
        android:gravity="center_vertical|center_horizontal"
        tools:layout_editor_absoluteX="216dp"
        tools:layout_editor_absoluteY="294dp" />

    <Button
        android:id="@+id/check_fees1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:text="CHECK FEES"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="104dp" />

    <Button
        android:id="@+id/check_results1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:text="CHECK\nRESULTS"
        tools:layout_editor_absoluteX="200dp"
        tools:layout_editor_absoluteY="104dp" />

    <Button
        android:id="@+id/course_materials1"
        android:layout_width="wrap_content"
        android:layout_height="45dp"
        android:background="@android:color/white"
        android:text="BOOKS"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="223dp" />

    <Button
        android:id="@+id/academic_calender1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:text="ACADEMIC\nCALENDER"
        tools:layout_editor_absoluteX="200dp"
        tools:layout_editor_absoluteY="220dp" />

    <Button
        android:id="@+id/send_message1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:text="SEND\nMESSAGE"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="344dp" />

    <Button
        android:id="@+id/logout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:text="LOGOUT"
        tools:layout_editor_absoluteX="200dp"
        tools:layout_editor_absoluteY="344dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="Hello"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="133dp"
        tools:layout_editor_absoluteY="7dp" />

</android.support.constraint.ConstraintLayout>